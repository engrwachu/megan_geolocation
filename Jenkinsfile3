pipeline{
    agent {
        docker {image 'node:20.10.0-alphine.19'}
    }
    stages{
        stage('check current directory'){
            steps{
                sh 'pwd'
                sh 'whoami'
                sh "mkdir \$(date '+%Y%m%d_%H%M%S')"
                sh 'python3 --version'
                sh 'npm --version'
            }
        }
    }
}
